{
  "name": "cozy-indexer",
  "version": "0.0.1",
  "description": "indexer package for the cozy-data-system",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --bail --compilers coffee:coffee-script/register test/index.coffee",
    "benchmark": "mocha --bail --compilers coffee:coffee-script/register test/benchmark.coffee",
    "build": "coffee  --output lib --bare --compile src ",
    "lint": "npm run lint1 && npm run lint2",
    "lint1": "coffeelint -f coffeelint.json ./src/*.coffee",
    "lint2": "coffee-jshint -o node ./src/*.coffee",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy-labs/cozy-indexer2"
  },
  "keywords": [
    "index",
    "search",
    "cozy",
    "indexer"
  ],
  "author": "Cozy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cozy-labs/cozy-indexer2/issues"
  },
  "homepage": "https://github.com/cozy-labs/cozy-indexer2",
  "dependencies": {
    "async": "1.4.2",
    "natural": "0.2.1",
    "franc": "1.1.1",
    "search-index": "0.6.11"
  },
  "devDependencies": {
    "coffeelint": "1.6.0",
    "coffee-19-jshint": "2.3.1",
    "coffee-script": "^1.10.0",
    "mocha": "^2.3.3",
    "should": "^7.1.0"
  }
}
